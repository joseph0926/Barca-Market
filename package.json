  "license": "MIT",
  "main": "index.js",
  "description": "Shared library for Barca app",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build-all": "npm run build-gateway && npm run build-notification && npm run build-auth && npm run build-user && npm run build-gig && npm run build-chat && npm run build-order && npm run build-review",
    "build-gateway": "cd packages/gateway-service && npm run build",
    "build-notification": "cd packages/notification-service && npm run build",
    "build-auth": "cd packages/auth-service && npm run build",
    "build-user": "cd packages/user-service && npm run build",
    "build-gig": "cd packages/gig-service && npm run build",
    "build-chat": "cd packages/chat-service && npm run build",
    "build-order": "cd packages/order-service && npm run build",
    "build-review": "cd packages/review-service && npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "name": "@joseph0926/barca-shared",
  "author": "joseph0926",
  "private": true,
  "workspaces": [
    "/packages/*"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
